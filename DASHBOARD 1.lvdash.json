{
  "datasets": [
    {
      "name": "5de699b9",
      "displayName": "silverbusiness1",
      "queryLines": [
        "SELECT\r\n",
        "b.name,\r\n",
        "b.city,\r\n",
        "b.state,\r\n",
        "AVG(r.stars) as avg_rating,\r\n",
        "COUNT(r.review_id) as review_count\r\n",
        "FROM workspace.default.silverbusiness1 b\r\n",
        "JOIN workspace.default.silverreview1 r ON b.business_id = r.business_id\r\n",
        "GROUP BY b.name, b.city, b.state\r\n",
        "HAVING COUNT(r.review_id) > 100\r\n",
        "ORDER BY review_count \r\n",
        "LIMIT 20;"
      ]
    },
